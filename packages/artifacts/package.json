{
    "name": "@ai-sdk-tools/artifacts",
    "private": false,
    "version": "0.1.0",
    "description": "Artifacts for AI SDK Tools",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    },
    "files": [
      "dist",
      "README.md",
      "LICENSE"
    ],
    "scripts": {
      "build": "tsup",
      "dev": "tsup --watch",
      "clean": "rm -rf dist",
      "prepublishOnly": "bun run clean && bun run build",
      "type-check": "tsc --noEmit"
    },
    "keywords": [
      "ai",
      "artifacts",
      "debugging",
      "chat",
      "streaming",
      "development",
      "ai-sdk",
      "react"
    ],
    "author": "Pontus Abrahamsson",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/midday-ai/ai-sdk-tools.git"
    },
    "bugs": {
      "url": "https://github.com/midday-ai/ai-sdk-tools/issues"
    },
    "homepage": "https://github.com/midday-ai/ai-sdk-tools#readme",
    "publishConfig": {
      "access": "public"
    }
  }
  